{
  "name": "@qbs-autonaim/emails",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    ".react-email"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./send": {
      "types": "./dist/send.d.mts",
      "default": "./dist/send.mjs"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "bun with-env email dev -p 3002",
    "build": "tsdown",
    "export": "email export",
    "with-env": "bun run --env-file ../../.env"
  },
  "dependencies": {
    "@radix-ui/colors": "3.0.0",
    "@react-email/components": "1.0.4",
    "@qbs-autonaim/config": "workspace:*",
    "@qbs-autonaim/tsconfig": "workspace:*",
    "@tailwindcss/typography": "0.5.19",
    "nodemailer": "^7.0.12",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-email": "5.2.1",
    "resend": "^6.7.0",
    "sharp": "^0.34.5",
    "tailwindcss": "4.1.18",
    "tailwindcss-animate": "1.0.7",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@react-email/preview-server": "5.2.1",
    "@types/node": "25.0.6",
    "@types/nodemailer": "^7.0.5",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "tsdown": "0.19.0",
    "typescript": "5.9.3"
  }
}
