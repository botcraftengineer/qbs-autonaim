{
  "name": "@qbs-autonaim/jobs",
  "private": true,
  "version": "0.1.1",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "default": "./src/inngest/client.ts"
    },
    "./channels": {
      "default": "./src/inngest/channels-client.ts"
    },
    "./auth": {
      "default": "./src/services/auth/index.ts"
    },
    "./lib/ai-client": {
      "default": "./src/lib/ai-client.ts"
    },
    "./services/buffer": {
      "default": "./src/services/buffer/index.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "bun with-env bun run --watch src/inngest/server.ts",
    "inngest": "bun run inngest-cli@latest dev -u http://localhost:8000/api/inngest",
    "hh:parse": "bun with-env bun run src/hh-response-parser.ts",
    "hh:enrich": "bun with-env bun run src/hh-enrich-responses.ts",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint --flag unstable_native_nodejs_ts_config",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "bun run --env-file ../../.env"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "2.0.4",
    "@ai-sdk/openai": "3.0.7",
    "@inngest/realtime": "0.4.5",
    "@qbs-autonaim/config": "workspace:*",
    "@qbs-autonaim/db": "workspace:*",
    "@qbs-autonaim/emails": "workspace:*",
    "@qbs-autonaim/lib": "workspace:*",
    "@qbs-autonaim/ai": "workspace:*",
    "@qbs-autonaim/shared": "workspace:*",
    "@qbs-autonaim/tg-client": "workspace:*",
    "@t3-oss/env-core": "0.13.10",
    "ai": "6.0.22",
    "axios": "1.13.2",
    "crawlee": "3.15.3",
    "inngest": "3.48.1",
    "langfuse": "3.38.6",
    "puppeteer": "24.34.0",
    "puppeteer-extra": "3.3.6",
    "puppeteer-extra-plugin-stealth": "2.11.2",
    "sharp": "0.34.5",
    "string-strip-html": "13.5.0",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@qbs-autonaim/tsconfig": "workspace:*",
    "@types/bun": "1.3.5",
    "@types/node": "25.0.3",
    "cross-env": "10.1.0",
    "inngest-cli": "^1.16.0",
    "tsdown": "0.18.4",
    "typescript": "5.9.3"
  }
}
