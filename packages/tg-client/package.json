{
  "name": "@qbs-autonaim/tg-client",
  "version": "0.1.2",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./sdk": {
      "types": "./dist/sdk/client.d.mts",
      "default": "./dist/sdk/client.mjs"
    },
    "./handlers/message-handler": {
      "types": "./src/handlers/message-handler.ts",
      "default": "./src/handlers/message-handler.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsdown && tsc --emitDeclarationOnly",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsdown --watch",
    "start": "bun run src/api/index.ts",
    "start:dev": "bun run --env-file ../../.env --watch src/api/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@mtcute/bun": "0.27.6",
    "@mtcute/dispatcher": "0.27.6",
    "@qbs-autonaim/config": "workspace:*",
    "@qbs-autonaim/db": "workspace:*",
    "@qbs-autonaim/lib": "workspace:*",
    "@qbs-autonaim/ai": "workspace:*",
    "@qbs-autonaim/shared": "workspace:*",
    "@qbs-autonaim/server-utils": "workspace:*",
    "file-type": "21.3.0",
    "hono": "4.11.3",
    "mime-types": "3.0.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@qbs-autonaim/tsconfig": "workspace:*",
    "@types/bun": "1.3.6",
    "@types/mime-types": "^3.0.1",
    "@types/node": "25.0.6",
    "tsdown": "^0.19.0",
    "typescript": "5.9.3"
  }
}
