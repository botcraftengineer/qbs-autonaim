{
  "name": "@qbs-autonaim/tg-client",
  "version": "0.1.2",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./sdk": {
      "types": "./dist/sdk/client.d.ts",
      "default": "./src/sdk/client.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "api": "bun run src/api/index.ts",
    "api:dev": "bun --watch src/api/index.ts",
    "bot": "bun run --env-file ../../.env src/start-bot.ts",
    "bot:dev": "bun --watch --env-file ../../.env src/start-bot.ts",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@mtcute/bun": "0.27.2",
    "@mtcute/dispatcher": "0.27.2",
    "@qbs-autonaim/config": "workspace:*",
    "@qbs-autonaim/db": "workspace:*",
    "@qbs-autonaim/lib": "workspace:*",
    "@qbs-autonaim/prompts": "workspace:*",
    "hono": "4.10.8",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@qbs-autonaim/tsconfig": "workspace:*",
    "@types/bun": "1.3.4",
    "@types/node": "25.0.0",
    "typescript": "5.9.3"
  }
}
