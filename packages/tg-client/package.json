{
  "name": "@qbs-autonaim/tg-client",
  "version": "0.1.2",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./sdk": {
      "types": "./dist/sdk/client.d.ts",
      "default": "./src/sdk/client.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "start": "bun run src/api/index.ts",
    "start:dev": "bun run --env-file ../../.env --watch src/api/index.ts",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@mtcute/bun": "0.27.4",
    "@mtcute/dispatcher": "0.27.4",
    "@qbs-autonaim/config": "workspace:*",
    "@qbs-autonaim/db": "workspace:*",
    "@qbs-autonaim/lib": "workspace:*",
    "@qbs-autonaim/prompts": "workspace:*",
    "file-type": "^21.1.1",
    "hono": "4.11.1",
    "mime-types": "^3.0.2",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@qbs-autonaim/tsconfig": "workspace:*",
    "@types/bun": "1.3.4",
    "@types/mime-types": "^3.0.1",
    "@types/node": "25.0.2",
    "typescript": "5.9.3"
  }
}
