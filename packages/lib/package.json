    "effect": "^3.19.12",
    "langfuse": "3.38.6",
    "zod": "4.2.1"
  },
  "optionalDependencies": {
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@qbs-autonaim/tsconfig": "workspace:*",
    "@types/node": "25.0.2",
    "tsdown": "0.18.0",
    "typescript": "5.9.3"
  }
