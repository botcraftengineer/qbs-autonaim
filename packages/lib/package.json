{
  "name": "@qbs-autonaim/lib",
  "version": "0.1.0",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./ai": {
      "types": "./dist/ai.d.mts",
      "default": "./dist/ai.mjs"
    },
    "./s3": {
      "types": "./dist/s3.d.mts",
      "default": "./dist/s3.mjs"
    },
    "./image": {
      "types": "./dist/image.d.mts",
      "default": "./dist/image.mjs"
    }
