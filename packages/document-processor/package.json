{
  "name": "@qbs-autonaim/document-processor",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "biome format .",
    "lint": "biome lint .",
    "check": "biome check .",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@qbs-autonaim/db": "workspace:*",
    "@qbs-autonaim/validators": "workspace:*",
    "llamaindex": "latest",
    "pgvector": "latest",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@qbs-autonaim/tsconfig": "workspace:*",
    "fast-check": "^4.5.3",
    "typescript": "5.9.3",
    "vitest": "^4.0.16"
  }
}
